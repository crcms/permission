{
  "name": "crcms/permission",
  "description": "The Permission Packages .",
  "keywords": [
    "permission",
    "laravel",
    "role",
    "field"
  ],
  "authors": [
    {
      "name": "doming",
      "email": "1396002924@qq.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.2.4",
    "crcms/foundation": "dev-master",
    "crcms/repository": "*",
    "illuminate/routing": "^5.7",
